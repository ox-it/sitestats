<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.sakaiproject.sitestats.impl.SiteActivityImpl"
		table="SST_SITEACTIVITY"
		lazy="true">
		<id name="id" type="long" column="ID" unsaved-value="0">
			<generator class="native">
				<param name="sequence">SST_SITEACTIVITY_ID</param>
			</generator>
		</id>
		<property name="siteId" column="SITE_ID" type="string" length="99" not-null="true" index="SITE_ID_IDX" />
		<property name="date" column="DATE" type="date" not-null="true" index="DATE_IDX" />
		<property name="eventId" column="EVENT_ID" type="string" length="32" not-null="true" index="EVENT_ID_IDX" />
		<property name="count" column="COUNT" type="long" not-null="true" />		
	</class>
</hibernate-mapping>
